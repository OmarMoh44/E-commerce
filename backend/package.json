{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for the project",
  "main": "app.js",
  "scripts": {
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "jest --watch",
    "build": "tsc --build",
    "start": "node -r module-alias/register ./dist/app.js",
    "start:dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "studio:dev": "npx prisma studio",
    "studio:test": "dotenv -e .env.test npx prisma studio",
    "seed": "ts-node src/prisma/seed.ts",
    "reset:dev": "npx prisma migrate reset",
    "reset:test": "dotenv -e .env.test npx prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "clearDB:dev": "ts-node src/prisma/clear.ts",
    "clearDB:test": "dotenv -e .env.test ts-node src/prisma/clear.ts",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "migrate:create": "npx prisma migrate dev --create-only"
  },
  "prisma": {
    "seed": "ts-node ./src/prisma/seed.ts",
    "schema": "src/prisma/schema/"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@faker-js/faker": "^9.2.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "firebase-admin": "^13.0.1",
    "google-auth-library": "^9.14.1",
    "graphql": "^16.11.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "phone": "^3.1.50",
    "randomstring": "^1.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testcontainers/postgresql": "^10.13.2",
    "@testcontainers/redis": "^10.13.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/hapi__joi": "^17.1.14",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.29.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-serial-runner": "^1.2.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "redis-mock": "^0.56.3",
    "supertest": "^7.0.0",
    "testcontainers": "^10.13.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.34.1"
  },
  "_moduleAliases": {
    "@controllers": "./dist/controllers",
    "@middlewares": "./dist/middlewares",
    "@DB": "./dist/prisma/PrismaClient",
    "@services": "./dist/services",
    "@validators": "./dist/validators",
    "@schema": "./dist/schema",
    "@models": "./dist/models",
    "@src": "./dist",
    "@test": "./dist/__tests__"
  }
}